<tool id='local.convert.gate2json_2.1.0' name='GateToLif v2.1.0' version='2.1.0'>
  <description>Converts GATE documents to the LAPPS JSON/LD format.</description>
  <command interpreter='lsd'>./invoke.lsd gate2json_2.1.0 $input $pretty $output</command>
  <inputs>
    <param name='input' type='data' format='gate' label='Gate Input' />
    <param name="pretty" type='boolean' checked='false' label='Pretty print'/>
  </inputs>
  <outputs>
    <data name='output' format='lif' />
  </outputs>
  <help>Converts GATE documents to the LAPPS JSON/LD format.</help>
</tool>